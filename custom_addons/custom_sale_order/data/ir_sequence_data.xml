<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Define the product sequence -->
        <record id="seq_product_internal_ref" model="ir.sequence">
            <field name="name">Product Internal Reference</field>
            <field name="code">product.internal.reference</field>
            <field name="prefix">PR-</field>
